<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sample.controllers.UserController">
   <children>
      <BorderPane fx:id="mainBorderPane" prefHeight="600.0" prefWidth="800.0">
         <top>
            <Label fx:id="LblTitle" alignment="CENTER" contentDisplay="CENTER" prefHeight="49.0" prefWidth="234.0" text="Title" BorderPane.alignment="CENTER">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <ButtonBar fx:id="UIButtonbar" prefHeight="38.0" prefWidth="450.0" BorderPane.alignment="CENTER">
               <buttons>
                  <Button fx:id="BtnPaintjobScene" mnemonicParsing="false" onAction="#showPaintjobScene" text="PAINTJOB" />
                  <Button fx:id="BtnGearboxScene" mnemonicParsing="false" onAction="#showGearboxScene" text="GEARBOX" />
                  <Button fx:id="BtnEngineScene" mnemonicParsing="false" onAction="#showEngineScene" text="ENGINE" />
                  <Button fx:id="BtnOverviewScene" disable="true" mnemonicParsing="false" onAction="#showOverviewScene" text="OVERVIEW" />
                  <Button fx:id="BtnSaveCar" mnemonicParsing="false" onAction="#SaveCar" text="SaveCar" />
                  <Button fx:id="BtnShowCars" mnemonicParsing="false" onAction="#ShowCars" text="Show cars" />
               </buttons>
            </ButtonBar>
         </bottom>
         <left>
            <VBox prefHeight="513.0" prefWidth="213.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="lblSelectPart" alignment="CENTER" prefHeight="17.0" prefWidth="229.0" text="Select part">
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Label>
                  <TilePane fx:id="PartSelectorTilePane" prefHeight="458.0" prefWidth="213.0" />
                  <Button fx:id="btnAddToConfiguration" alignment="BOTTOM_CENTER" disable="true" mnemonicParsing="false" onAction="#AddSelectedPart" prefHeight="27.0" prefWidth="214.0" text="Add to configuration">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </left>
         <right>
            <VBox prefHeight="513.0" prefWidth="195.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="233.0" text="Current Configuration">
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Label>
                  <Label alignment="TOP_LEFT" prefHeight="489.0" prefWidth="175.0" text="Current Setup" textAlignment="CENTER" />
               </children>
            </VBox>
         </right>
         <center>
            <VBox prefHeight="513.0" prefWidth="407.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="tableViewCars" editable="true" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="Engine" >
                       <cellValueFactory>
                          <PropertyValueFactory property="engine" />
                       </cellValueFactory>
                       </TableColumn>

                      <TableColumn prefWidth="75.0" text="Gearbox" >
                         <cellValueFactory>
                            <PropertyValueFactory property="gearbox" />
                         </cellValueFactory>
                      </TableColumn>

                       <TableColumn prefWidth="75.0" text="Paintjob" >
                          <cellValueFactory>
                             <PropertyValueFactory property="paintjob" />
                          </cellValueFactory>
                       </TableColumn>

                    </columns>
                  </TableView>
               </children></VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
